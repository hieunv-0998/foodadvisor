{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "name": "project"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    },
    "name": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "content": {
      "type": "richtext"
    },
    "slices": {
      "type": "dynamiczone",
      "components": [
        "shared.embed-form",
        "shared.subscribe-newsletter",
        "shared.embed-tweet"
      ]
    }
  }
}
